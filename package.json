    "gulp-shell": "0.4.2",
    "gulp-sourcemaps": "1.5.2",
    "gulp-uglify": "1.1.0",
    "gulp-util": "3.0.6",
    "hazmat": "*",
    "jest-cli": "0.4.18",
    "jquery": "2.1.3",
    "jsdom": "0.11.1",
    "mocha": "~1.8.2",
    "requirejs": "2.1.20",
    "underscore": "1.3.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "author": "Ooyala",
  "contributors": [
    "Peter Iliev <piliev@ooyala.com>",
